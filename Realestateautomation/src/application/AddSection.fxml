<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddSection">
   <children>
      <AnchorPane fx:id="anchorList" layoutX="7.0" layoutY="64.0" prefHeight="435.0" prefWidth="600.0">
         <children>
            <Pane prefHeight="435.0" prefWidth="600.0">
               <children>
                  <Label layoutX="392.0" layoutY="101.0" text="Square Meters" />
                  <Label layoutX="15.0" layoutY="185.0" text="Number of rooms" />
                  <Label layoutX="14.0" layoutY="225.0" text="Home Heating System" />
                  <Label layoutX="16.0" layoutY="268.0" text="Furnished" />
                  <Label layoutX="16.0" layoutY="329.0" text="Adress" />
                  <Label layoutX="383.0" layoutY="22.0" text="Price" />
                  <Label layoutX="20.0" layoutY="22.0" text="City" />
                  <Label layoutX="20.0" layoutY="77.0" text="Country" />
                  <Label layoutX="14.0" layoutY="139.0" text="District/ Neighborhood" />
                  <ComboBox fx:id="cmboSelectCity" layoutX="144.0" layoutY="18.0" onAction="#cmboSelectCityClick" prefWidth="150.0" promptText="Select City" styleClass="ComboBox" />
                  <ComboBox fx:id="cmboSelectCountry" layoutX="144.0" layoutY="73.0" onAction="#cmboSelectCountryClick" prefWidth="150.0" promptText="Select Country" styleClass="ComboBox" />
                  <ComboBox fx:id="cmboSelectDistrict" layoutX="144.0" layoutY="135.0" onAction="#cmboSelectDistrictClick" prefWidth="150.0" promptText="Select District/Neighborhood" styleClass="ComboBox" />
                  <ComboBox fx:id="cmboSelectNoR" layoutX="144.0" layoutY="181.0" onAction="#cmboSelectNoRClick" prefWidth="150.0" promptText="Select Number Of Rooms" styleClass="ComboBox" />
                  <ComboBox fx:id="cmboSelectHHS" layoutX="144.0" layoutY="221.0" onAction="#cmboSelectHHSClick" prefWidth="150.0" promptText="Select HHS" styleClass="ComboBox" />
                  <CheckBox fx:id="checkboxFurnished" layoutX="144.0" layoutY="268.0" mnemonicParsing="false" onAction="#checkboxFurnishedClick" text="Yes">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </CheckBox>
                  <TextArea fx:id="txtareaAdress" layoutX="144.0" layoutY="299.0" prefHeight="94.0" prefWidth="214.0" promptText="Adress" />
                  <TextField fx:id="txtPrice" layoutX="359.0" layoutY="52.0" prefHeight="25.0" prefWidth="74.0" promptText="Price" />
                  <TextField fx:id="txtSqrMeters" layoutX="361.0" layoutY="127.0" prefHeight="25.0" prefWidth="95.0" promptText="Square Meters" />
                  <DatePicker fx:id="datepickerBuildAge" layoutX="358.0" layoutY="184.0" promptText="Building Age" />
                  <RadioButton fx:id="radioforSell" layoutX="366.0" layoutY="238.0" mnemonicParsing="false" onAction="#radioforSellClick" text="For Sell" />
                  <RadioButton fx:id="radioforRent" layoutX="458.0" layoutY="238.0" mnemonicParsing="false" onAction="#radioforRentClick" text="For Rent" />
               </children>
            </Pane>
         </children>
      </AnchorPane>
      <Pane layoutX="1.0" layoutY="514.0" prefHeight="127.0" prefWidth="300.0">
         <children>
            <Button fx:id="btnSave" layoutX="19.0" layoutY="5.0" mnemonicParsing="false" onAction="#btnSaveClick" prefHeight="108.0" prefWidth="265.0" text="Save" />
         </children>
      </Pane>
      <Pane layoutX="314.0" layoutY="514.0" prefHeight="127.0" prefWidth="300.0">
         <children>
            <Button fx:id="btn_addPicture" layoutX="19.0" layoutY="5.0" mnemonicParsing="false" onAction="#btn_addPictureClick" prefHeight="108.0" prefWidth="265.0" text="Go Back" />
         </children>
      </Pane>
   </children>
</AnchorPane>
